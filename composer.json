{
    "name": "erol-simsir/csv-console-reader",
    "description": "",
    "type": "project",
    "require": {
        "symfony/console": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test:unit": "./vendor/bin/phpunit tests",
        "test:1": "php ./bin/csv-console-reader render ./resources/test1.csv",
        "test:2": "php ./bin/csv-console-reader render ./resources/test2.csv",
        "test:3": "php ./bin/csv-console-reader render ./resources/test3.csv"
    },
    "bin": [
        "bin/csv-console-reader"
    ],
